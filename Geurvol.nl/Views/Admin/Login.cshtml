
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-md-4"> < < @Html.ActionLink("Terug naar website", "Index", "Home")</div>
            <div class="col-md-4">
                <div class="form-group">
                    <div class="left-inner-addon">
                        <i class="icon-user"></i>
                        <input type="text"
                               class="form-control"
                               placeholder="Username" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="left-inner-addon">
                        <i class="icon-user"></i>
                        <input type="password"
                               class="form-control"
                               placeholder="Password" />
                    </div>
                </div>

                <button class="btn btn-primary">Inloggen</button>
            </div>
            <div class="col-md-4"></div>
        </div>
    </div>
</body>
</html>
